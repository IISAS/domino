server {
    listen 80;
    server_name localhost;

    root /usr/share/nginx/html;

    location = /domino {
        return 301 /domino/;
    }

    location /domino/assets/ {
	    alias /usr/share/nginx/html/assets/;
    }

    location /domino/ {
        alias /usr/share/nginx/html/;
        index index.html;
        try_files $uri $uri/ /index.html;
    }
}

